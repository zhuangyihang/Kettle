           </partitioning>
    <connection>pgy_dw</connection>
    <sql>SELECT 
rm.CODE AS ROOM_CODE,
rm.STORE_CODE AS STORE_CODE,
!rm.IS_VALID AS REMOVED,
rm.BUSINESS_DISTRICT AS BUSINESS_DISTRICT,
rm.FLOOR AS FLOOR,
rm.HEIGHT AS HEIGHT,
rm.LAT AS LAT,
rm.LNG AS LNG,
rm.MAX_CAPACITY AS MAX_CAPACITY,
rm.OPERATION_MANAGER AS OPERATION_MANAGER,
rm.STORE_NAME AS STORE_NAME,
rm.NOTES AS NOTES,
rm.IS_VIRTUAL AS IS_VIRTUAL,
info.NAME AS DISPLAY_NAME ,
rm.IS_VALID AS IS_VALID
FROM 
${TABLE_PGY}.T_ROOM AS rm
INNER JOIN ${TABLE_PGY}.T_ROOM_I18N_INFO AS info
ON info.INT_VALUE = rm.ID
WHERE 
rm.REMOVED=0 
 AND 
(rm.CREATE_TIME&gt;(
SELECT ${TABLE_PGY_DW}.T_TABLE_CONFIG.EXTRA_TIME FROM T_TABLE_CONFIG where TABLE_NAME =&apos;T_DIM_ROOM&apos; )
OR 
rm.LAST_UPDATE_TIME&gt;(
SELECT ${TABLE_PGY_DW}.T_TABLE_CONFIG.EXTRA_TIME FROM T_TABLE_CONFIG where TABLE_NAME =&apos;T_DIM_ROOM&apos; ))
and 
(
rm.CREATE_TIME&lt;&apos;${sysTime}&apos;
OR 
rm.LAST_UPDATE_TIME&lt;&apos;${sysTime}&apos;
)
</sql>
